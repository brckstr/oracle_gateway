[Unit]

Description=Gunicorn Daemon for FastAPI example

After=network.target


[Service]

User=ubuntu

Group=www-data

WorkingDirectory=/home/oracle_gateway

ExecStart= source/home/oracle_gateway/bin/activate;/home/oracle_gateway/bin/gunicorn -c gunicorn_conf.py main:app


[Install]

WantedBy=multi-user.target
